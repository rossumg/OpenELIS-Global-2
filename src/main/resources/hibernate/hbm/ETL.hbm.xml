<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="org.openelisglobal.etl.valueholder.ETLRecord"
		table="ETL" optimistic-lock="version"
		dynamic-update="true">
		<id name="id"
			type="org.openelisglobal.hibernate.resources.usertype.LIMSStringNumberUserType">
			<column name="ID" precision="10" scale="0" />
			<generator
				class="org.openelisglobal.hibernate.resources.StringSequenceGenerator">
				<param name="sequence_name">etl_seq</param>
			</generator>
		</id>
		<version name="lastupdated" column="LASTUPDATED"
			type="timestamp" access="field" />
		<property name="orderTimestamp" type="java.sql.Timestamp">
			<column name="ORDER_TIMESTAMP" length="7" not-null="true" />
		</property>
		<property name="externalId" type="java.lang.String">
			<column name="EXTERNAL_ID" length="60" not-null="true" />
		</property>
		<many-to-one name="patient"
			class="org.openelisglobal.patient.valueholder.Patient" fetch="select" lazy="false">
			<column name="patient_id" precision="10" scale="0" />
		</many-to-one>
		<property name="statusId"
			type="org.openelisglobal.hibernate.resources.usertype.LIMSStringNumberUserType"
			column="status_id" precision="10" not-null="true" />
		<property name="data" type="java.lang.String">
			<column name="data" not-null="true" />
		</property>

	<property name="labno" type="java.lang.String">
		<column name="labno" not-null="true" />
	</property>
	<property name="identifier" type="java.lang.String">
		<column name="identifier" not-null="true" />
	</property>
	
	<property name="first_name" type="java.lang.String">
		<column name="first_name" not-null="true" />
	</property>
	<property name="last_name" type="java.lang.String">
		<column name="last_name" not-null="true" />
	</property>
	<property name="home_phone" type="java.lang.String">
		<column name="home_phone" not-null="true" />
	</property>
	<property name="work_phone" type="java.lang.String">
		<column name="work_phone" not-null="true" />
	</property>
	
	<property name="sex" type="java.lang.String">
		<column name="sex" not-null="true" />
	</property>
	<property name="birthdate" type="java.sql.Timestamp">
		<column name="birthdate" length="7" not-null="true" />
	</property>
	
	<property name="address_street" type="java.lang.String">
		<column name="address_street" not-null="true" />
	</property>
	<property name="address_city" type="java.lang.String">
		<column name="address_city" not-null="true" />
	</property>
	<property name="address_country" type="java.lang.String">
		<column name="address_country" not-null="true" />
	</property>
	
	
	<property name="age_years" 
		type="java.lang.Integer"
		column="age_years" precision="10" not-null="true" />
	<property name="age_months" 
		type="java.lang.Integer"
		column="age_months" precision="10" not-null="true" />
	<property name="age_weeks" 
		type="java.lang.Integer"
		column="age_weeks" precision="10" not-null="true" />
	
	
	<property name="date_recpt" type="java.sql.Timestamp">
		<column name="date_recpt" length="7" not-null="true" />
	</property>
	<property name="date_entered" type="java.sql.Timestamp">
		<column name="date_entered" length="7" not-null="true" />
	</property>
	<property name="date_collect" type="java.sql.Timestamp">
		<column name="date_collect" length="7" not-null="true" />
	</property>
	
	<property name="code_referer" type="java.lang.String">
		<column name="code_referer" not-null="true" />
	</property>
	<property name="referer" type="java.lang.String">
		<column name="referer" not-null="true" />
	</property>
	<property name="program" type="java.lang.String">
		<column name="program" not-null="true" />
	</property>
	<property name="order_status" type="java.lang.String">
		<column name="order_status" not-null="true" />
	</property>
	<property name="test" type="java.lang.String">
		<column name="test" not-null="true" />
	</property>
	<property name="result" type="java.lang.String">
		<column name="result" not-null="true" />
	</property>
		
	</class>
</hibernate-mapping>
